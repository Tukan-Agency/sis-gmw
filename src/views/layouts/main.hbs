<script>
  function truncarTexto() {
  var contenedor = document.getElementById("parrafou");
  var texto = contenedor.innerHTML;
  var longitudMaxima = 100;
  
  if (texto.length > longitudMaxima) {
    texto = texto.substring(0, longitudMaxima) + "...";
  }
  
  contenedor.innerHTML = texto;
}
truncarTexto();
</script>
 
