{{#if Gatewa}}
 <div class="table-de ">
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>N°</th>
        <th>Proyecto</th>
        <th>Conexión</th>
        <th>Tipo de envio</th>
        <th>Mensaje</th>
        <th>Imágen</th>
        <th>codigo</th>
        <th>Acción</th>
      </tr>
    </thead>
    <tbody>
      {{#each Gatewa}}
        <tr>
          <td>{{@index}}</td>
          <td><strong>{{proyecto}}</strong></td>
          <td>{{conexion}}</td>
          <td>{{tipo_envio}}</td>
           <td ><p id="parrafou" class="msge" >{{text}}</p></td>
           <td> <img class="img-tabl" src="{{url_imagen}}" alt=""></td>
          <td><p  class="cdes">{{codigo}}</p></td>
          <td>
            {{#if done}}
               
            {{/if}}
            <a href="/tasks/{{_id}}/delete" class="btn btn-outline-danger">
              Delete
            </a>
            <a href="/tasks/{{_id}}/edit" class="btn btn-outline-info">
              Edit
            </a>
           
            
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
  </div>
{{else}}
  <div class="h-100 d-flex align-items-center justify-content-center">
    <div class="text-center ">
       <div class="div-vacio">
        <h3>Agrega un nuevo proyecto</h3>
      <img src="/icons/empty.svg" class="w-75" />
      </div>
    </div>


  </div>

{{/if}}